#catucaPai#
bebel:i;
bebel:j;
i=0;
j=i;
lineu|i<3|{;
    beicola |i == 0| {;
        carrarinhaln|"Zero"|;
    };
    etelvina {;
        carrarinhaln|"Outro"|;
        i=i+1;
    };
};
beicola|i==3|{;
    beicola|j==0| {;
        carrarinhaln|"Ok!"|;
    };
    etelvina {;
        carrarinhaln|"Erro"|;
    };
};
#catucaMae#